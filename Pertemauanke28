package zaskia;

public class pertemuanke28 {
    
    public static void main(String[] args) {
        
        byte angka = 3;
        
        if (angka>10) {
        System.out.println("angka diatas 10");
        
    }else if(angka<=10) { 
        System.out.println("angka dibawah dari 10");
        
    }else{ 
        System.out.println("angka terserah");
        
        
        
        
    }
        
        
    }
    
}
