package zaskia;

import java.math.BigInteger;

public class pertemuanke15 {
    
    public static void main(String[] args) {
        
        //deklarasi program
        
        BigInteger nilaiA = new BigInteger ("23456789101112131415");
        BigInteger nilaiB = new BigInteger ("3456789101112131415161718");
        BigInteger nilaiC = new BigInteger ("12345678910112021");
        
        //proses
        
        BigInteger hasiltambah = nilaiA.add(nilaiB);
        BigInteger hasilkurang = nilaiA.subtract(nilaiB);
        BigInteger hasilkali = nilaiA.multiply(nilaiB);
        BigInteger hasilbagi = nilaiA.divide(nilaiB);
        
        //output
        
        System.out.println("nilai\t :"+hasiltambah);
        System.out.println("nilai\t :"+hasilkurang);
        System.out.println("nilai\t :"+hasilkali);
        System.out.println("nilai\t :"+hasilbagi);
        
        
